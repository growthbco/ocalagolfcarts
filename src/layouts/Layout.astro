    <!-- Mobile Menu JavaScript -->
    <script>
      // @ts-ignore
      // Mobile menu toggle functionality
      function toggleMobileMenu() {
        const menu = document.getElementById('mobile-menu');
        const button = document.getElementById('mobile-menu-button');
        const isOpen = menu?.classList.contains('translate-y-0');
        
        if (menu) {
          menu.classList.toggle('translate-y-0');
          menu.classList.toggle('-translate-y-full');
        }
        
        // Change hamburger to X when menu is open
        if (button) {
          const svg = button.querySelector('svg');
          if (svg) {
            if (!isOpen) {
              // Change to X icon
              svg.innerHTML = '<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"></path>';
            } else {
              // Change back to hamburger
              svg.innerHTML = '<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16"></path>';
            }
          }
        }
      }

      // @ts-ignore
      // Mobile dropdown toggle functionality
      function toggleMobileDropdown(dropdownId: string) {
        const dropdown = document.getElementById(dropdownId + '-dropdown');
        const chevron = document.getElementById(dropdownId + '-chevron');
        
        if (dropdown) {
          dropdown.classList.toggle('hidden');
        }
        if (chevron) {
          chevron.classList.toggle('rotate-180');
        }
      }

      // Add click event listeners
      document.addEventListener('DOMContentLoaded', function() {
        const button = document.getElementById('mobile-menu-button');
        if (button) {
          button.addEventListener('click', toggleMobileMenu);
        }
        
        // Add click handlers for mobile dropdowns
        const rentalsToggle = document.getElementById('rentals-toggle');
        const cartsToggle = document.getElementById('carts-toggle');
        const eventsToggle = document.getElementById('events-toggle');
        
        if (rentalsToggle) {
          rentalsToggle.addEventListener('click', function() {
            toggleMobileDropdown('rentals');
          });
        }
        
        if (cartsToggle) {
          cartsToggle.addEventListener('click', function() {
            toggleMobileDropdown('carts');
          });
        }
        
        if (eventsToggle) {
          eventsToggle.addEventListener('click', function() {
            toggleMobileDropdown('events');
          });
        }
        
        // Close mobile menu when clicking outside
        document.addEventListener('click', function(event) {
          const menu = document.getElementById('mobile-menu');
          const button = document.getElementById('mobile-menu-button');
          const target = event.target as Node;
          
          if (menu && button && !menu.contains(target) && !button.contains(target)) {
            if (menu.classList.contains('translate-y-0')) {
              toggleMobileMenu();
            }
          }
        });
      });
    </script>
